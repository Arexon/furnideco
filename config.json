{
	"type": "minecraftBedrock",
	"name": "FurniDeco",
	"namespace": "furnideco",
	"authors": [
		"Arexon"
	],
	"targetVersion": "1.18.10",
	"description": "Adds 25 pieces of functional, decorative and unique furniture that fit with the Minecraft vanilla style.",
	"experimentalGameplay": {
		"enableGameTestFramework": false,
		"additionalModdingCapabilities": false,
		"cavesAndCliffs": false,
		"holidayCreatorFeatures": true,
		"creationOfCustomBiomes": false,
		"upcomingCreatorFeatures": true,
		"experimentalMolangFeatures": true,
		"educationEdition": false
	},
	"packs": {
		"behaviorPack": "./BP",
		"resourcePack": "./RP"
	},
	"compiler": {
		"plugins": [
			"typeScript",
			"entityIdentifierAlias",
			"customEntityComponents",
			"customItemComponents",
			"customBlockComponents",
			"customCommands",
			"moLang",
			[
				"simpleRewrite",
				{
					"packName": "FurniDeco"
				}
			]
		]
	}
}